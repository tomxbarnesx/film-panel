<h1>Edit Instructor Profile</h1>

<%= form_for @instructor do |f|%>
    <%= fields_for :profile, @instructor.profile do |p| %>
        <%= p.text_field :first_name %>
        <%= p.text_field :last_name %>
        <%= p.text_field :email %>
        <%= p.text_field :photo_url %>
        <%= p.text_field :password %>
    <% end %>
    <%= f.text_field :salary %>
    <%= f.text_field :edu %>
    <%= f.button :submit %>
<% end %>